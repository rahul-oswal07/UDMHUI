<?xml version="1.0"?>
<configuration>
	<connectionStrings>

		<!-- This configuration file sets up the PATHs & variables required for package execution  -->

		 <!--Database Connection String  -->
	  <add name="ConnectionString" connectionString="Data Source=localhost;Initial Catalog=PHOENIX_UDMH_RB_TEST;Integrated Security=SSPI;" providerName="System.Data.SqlClient"/>

		<!-- Variable to hold SSIS Packages Location -->
		<add name="PkgPath" connectionString="C:\Unisys\UDTS\Packages\"></add>

		<!-- Variable to hold Priority value  -->
		<add name="Priority" connectionString="10"></add>

		<!-- Provide choices of values in comments like â€˜SOURCE, TARGET, CONTROL  -->
