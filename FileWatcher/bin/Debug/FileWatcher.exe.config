<?xml version="1.0"?>
<configuration>
  <appSettings>

    <!-- Variable to hold DB name to log audit message -->
    <add key="ConnectionString" value="Data Source=localhost;Initial Catalog=PHOENIX_UDMH_DEV; Integrated Security=SSPI; "/>
    
    <!-- Variable to hold the path of FileLandingArea   -->
    <add key="FileLandingArea" value="D:\Input\filewatcher"/>

    <!-- Variable to hold back up folder path -->
    <add key="Save" value="D:\Input\Save"/>

    <!-- Variable to hold file processing folder path (Should be same as  STM RawfilePath without "\")-->
    <add key="Processed" value="D:\DATA\PHOENIX\Input\Raw"/>

    <!-- Variable to hold SDM application exe path  -->
    <add key="SDMEXEPath" value="C:\Unisys\UDTS\Components\SourceTransformationManager.exe"/> 

    <!-- Variable to enable/disable log -->
    <add key="EnableLog" value="True"/>

  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
